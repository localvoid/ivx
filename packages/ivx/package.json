{
  "name": "ivx",
  "main": "dist/cjs/index.js",
  "module": "dist/js/index.js",
  "typings": "dist/typings/index.d.ts",
  "sideEffects": false,
  "version": "0.1.0",
  "description": "Component-based HTML renderer",
  "license": "MIT",
  "author": {
    "name": "Boris Kaul",
    "email": "localvoid@gmail.com",
    "url": "https://github.com/localvoid"
  },
  "keywords": [
    "ivx",
    "html",
    "virtualdom",
    "vdom"
  ],
  "homepage": "https://github.com/localvoid/ivx",
  "repository": {
    "type": "git",
    "url": "https://github.com/localvoid/ivx"
  },
  "bugs": {
    "url": "https://github.com/localvoid/ivx/issues"
  },
  "github": "https://github.com/localvoid/ivx",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prepublish": "npm run dist",
    "dist": "npm run dist:es6 && npm run dist:cjs",
    "dist:es6": "../../node_modules/.bin/tsc --project ./tsconfig.build.es6.json",
    "dist:cjs": "../../node_modules/.bin/tsc --project ./tsconfig.build.cjs.json",
    "lint": "../../node_modules/.bin/tslint --project ./tsconfig.json",
    "test": "../../node_modules/.bin/jest",
    "test:coverage": "../../node_modules/.bin/jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*\\.spec)\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
